<div class="movie-banner">
  <h1 class="text-center">My Movie Lists</h1>
</div>
<div class="mx-5 my-3">
<%= link_to "Add new list", new_list_path, class:"btn btn-primary mb-2" %>
<div class="row">
  <% @lists.each do |list| %>
   <div class="col-md-4 card-category d-flex justify-content-center"
     style="background-image: url('<%= cl_image_path list.photo.key%>')">
    <%= link_to list.name, list_path(list) %>
   </div>
  <% end %>
</div>
</div>
